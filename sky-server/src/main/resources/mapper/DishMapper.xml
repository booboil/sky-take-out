<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.mapper.DishMapper">


    <insert id="insert">
        insert into dish (name,category_id,price,image,description,create_time,create_user,update_time,update_user,status)
        values (#{name},#{categoryId},#{price},#{image},#{description},#{createTime},#{createUser},#{updateTime},#{updateUser},#{status})
    </insert>
</mapper>
